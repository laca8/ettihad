{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "iad1"
    ],
    "functions": {
        "src/api/*.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}